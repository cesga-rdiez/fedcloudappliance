# OpenStack AUTH variables
export OS_AUTH_URL=${AUTH_URL}
export OS_PROJECT_DOMAIN_ID=${PROJECT_DOMAIN_NAME}
export OS_REGION_NAME=RegionOne
export OS_USER_DOMAIN_ID=${USER_DOMAIN_NAME}
##export OS_PROJECT_NAME="<your project>"
export OS_IDENTITY_API_VERSION=3
export OS_USERNAME=${USERNAME}
export OS_PASSWORD=${PASSWORD}
export OS_AUTH_TYPE=password

# AMS cert
export HOSTCERT=/etc/grid-security/hostcert.pem
export HOSTKEY=/etc/grid-security/hostkey.pem
